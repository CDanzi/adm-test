    <rule>
        <name>CASE_APP_updateCustomer</name>
        <uuid>14712107-e055-44d4-bd64-8bdfffb842a7</uuid>
        <description>Initial SAIL form for CASE_APP Update Customer process </description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5839</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(&#xD;
  local!adminUser:loggedInUser(),&#xD;
  local!collabUsrList,&#xD;
  #"SYSTEM_SYSRULES_formLayout"(&#xD;
    label: "Update Customer: " &amp; ri!customerData.name,&#xD;
    firstColumnContents: {&#xD;
      #"d3b95f27-f45a-4d97-812f-ef8de3681197"(&#xD;
        customerData: ri!customerData,&#xD;
        logo: ri!logo,&#xD;
        isCreate: ri!isCreate,&#xD;
        isShippingBillingSame: ri!isShippingBillingSame&#xD;
      )&#xD;
    },&#xD;
    buttons: #"SYSTEM_SYSRULES_ButtonLayout"(&#xD;
      primaryButtons: {&#xD;
        #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
          label: "Submit",&#xD;
          style: "PRIMARY",&#xD;
          saveInto:{&#xD;
            ri!button,&#xD;
            a!save(target: ri!customerData.inactiveFlag, value:false),&#xD;
            a!save(ri!customerData.CustomerSince, todate(now())),&#xD;
            a!save(ri!customerData.CreatedOn, now()),&#xD;
            a!save(ri!customerData.CreatedBy, loggedInUser()),&#xD;
          },&#xD;
          value: "submit"&#xD;
        ),&#xD;
        #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
          label: "Cancel",&#xD;
          value: "cancel",&#xD;
          saveInto: ri!button,&#xD;
          skipValidation: true&#xD;
        )&#xD;
      }&#xD;
    )&#xD;
  )&#xD;
)</definition>
