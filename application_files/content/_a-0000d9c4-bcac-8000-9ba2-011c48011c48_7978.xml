<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_25024</versionUuid>
    <rule>
        <name>CASE_APP_ICM_createAdHocTask</name>
        <uuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_7978</uuid>
        <description>Creates an AD Hoc Task object for the specified milestone in the ICM Data Object</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2260</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= 'type!{urn:com:appian:types:CASE_APP}CASE_APP_AdHocTasking'(
  taskName: ri!icmData.milestones[ri!index].name,
  taskDescription: ri!icmData.milestones[ri!index].description,
  assignees:{
    append(
      apply('type!{urn:com:appian:types:CASE_APP}CASE_APP_Assignee'(username:_),ri!icmData.collaborators.username),
      'type!{urn:com:appian:types:CASE_APP}CASE_APP_Assignee'(username:ri!icmData.owner)
    )
  },
  status: #"_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5862"[1],
  requestDate: now(),
  requestUser: ri!icmData.owner,
  deadline: ri!icmData.milestones[ri!index].dueDate,
  parentProcessID: ri!parentProcessId,
  parentCaseType:"Investigation",
  entryId:ri!entryId,
  taskType: ri!taskType
)</definition>
        <namedTypedValue>
            <name>icmData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>parentProcessId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>entryId</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>taskType</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
