<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_32754</versionUuid>
    <rule>
        <name>CASE_APP_ICM_uiGetMilestoneIndicator</name>
        <uuid>c2fa3c93-1715-4974-8997-e79cde78c922</uuid>
        <description>Returns an image field representing a check or ex depending on whether the passed in milestones have been completed. </description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2260</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_imageField"(
  label:ri!milestoneData.name[ri!index],
  labelPosition:"ADJACENT",
  images:#"SYSTEM_SYSRULES_documentImage"(
    document:#"SYSTEM_SYSRULES_iconIndicator"(if(#"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1982"(ri!completedMilestoneNames),
      "STATUS_NOTDONE",
      if(contains(ri!completedMilestoneNames,ri!milestoneData.name[ri!index]),
        "STATUS_OK",
        "STATUS_NOTDONE"
      )
    )
  )),
  size:"ICON"
)</definition>
        <namedTypedValue>
            <name>milestoneData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>completedMilestoneNames</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
