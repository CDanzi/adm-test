<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_30733</versionUuid>
    <rule>
        <name>CASE_APP_getEntityFieldsById</name>
        <uuid>_a-0000dd7a-9e3c-8000-3428-01ef9001ef90_6597</uuid>
        <description>Receives an entity id, entity type, and a list of fields to query</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1981</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!data: if(
    isnull(ri!entityId),
    null,
    #"SYSTEM_SYSRULES_queryEntity"(
      entity: ri!entityType,
      query: #"SYSTEM_SYSRULES_query"(
        selection: #"SYSTEM_SYSRULES_querySelection"(apply(
          #"SYSTEM_SYSRULES_queryColumn"(field: _),
          ri!fields
        )),
        filter: #"SYSTEM_SYSRULES_queryFilter"("id", "=", ri!entityId),
        pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(1, 1, null)
      )
    )
  ),
  if(
    isnull(local!data),
    null,
    if(
      local!data.totalCount &gt; 0,
      local!data.data[1],
      null
    )
  )
)</definition>
        <namedTypedValue>
            <name>entityType</name>
            <type>
                <name>DataStoreEntity</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>entityId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>fields</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
