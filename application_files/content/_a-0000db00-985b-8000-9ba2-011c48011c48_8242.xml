<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_25042</versionUuid>
    <rule>
        <name>CASE_APP_ICM_updateMilestoneTasks</name>
        <uuid>_a-0000db00-985b-8000-9ba2-011c48011c48_8242</uuid>
        <description>Appends the newly generated Milestone tasks to the already completed ones.</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2260</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(
  local!completedMilestoneTasks: ri!milestoneTasksData[where(apply(
    #"_a-0000db00-985b-8000-9ba2-011c48011c48_8238",
    ri!milestoneTasksData
  ))],
  local!completedMilestones: ri!milestones[wherecontains(
    local!completedMilestoneTasks.taskName, ri!milestones.name)
  ],
  local!currentMilestones: difference(ri!milestones, local!completedMilestones),
  local!tmpIcmData: 'type!{urn:com:appian:types:CASE_APP}CASE_APP_ICM_Data'(
    owner: ri!owner,
    collaborators: ri!collaborators,
    milestones: local!currentMilestones
  ),
  local!newMilestoneTasks: #"_a-0000d9c4-bcac-8000-9ba2-011c48011c48_7979"(    
    local!tmpIcmData, ri!processId, ri!entryId
  ),
  local!typeId: typeof({'type!{urn:com:appian:types:CASE_APP}CASE_APP_AdHocTasking'()}),
  append(cast(local!typeId, local!completedMilestoneTasks), cast(local!typeId, local!newMilestoneTasks))
)</definition>
        <namedTypedValue>
            <name>owner</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>collaborators</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>milestones</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>milestoneTasksData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>processId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>entryId</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
