<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_29860</versionUuid>
    <rule>
        <name>CASE_APP_formatCCNumber</name>
        <uuid>_a-0000dc96-9a5e-8000-6a17-01ef9001ef90_4770</uuid>
        <description>Formats a credit card number for display purposes. Will only format 16 or 15 digit credit cards numbers. Everything else will be returned as it was.</description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5996</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  len(ri!ccNumber) = 15,
  left(ri!ccNumber, 4) &amp; "-" &amp; mid(ri!ccNumber, 5, 6) &amp; "-" &amp; mid(ri!ccNumber, 11, 5),
  if(
    len(ri!ccNumber) = 16,
    left(ri!ccNumber, 4) &amp; "-" &amp; mid(ri!ccNumber, 5, 4) &amp; "-" &amp; mid(ri!ccNumber, 9, 4)&amp; "-" &amp; mid(ri!ccNumber, 13, 4),
    ri!ccNumber
  )
)</definition>
        <namedTypedValue>
            <name>ccNumber</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
