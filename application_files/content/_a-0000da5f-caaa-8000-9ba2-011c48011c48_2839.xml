    <rule>
        <name>CASE_APP_SR_getListView</name>
        <uuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2839</uuid>
        <description>Generates the listview definition for the Process Backed Service Request record.</description>
        <parentUuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2617</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=#"SYSTEM_SYSRULES_listViewItem"(
  title: "Service Request #" &amp; rf!serviceReqData.requestNum &amp; " - " &amp; rf!serviceReqData.type,
  details: "Requested by: " &amp; rf!serviceReqData.requesterName &amp; "
Related Customer: " &amp; rf!serviceReqData.relatedCustomer.Name &amp; "
Status: " &amp; #"_a-0000da5f-caaa-8000-9ba2-011c48011c48_2630"[rf!currentMilestone] &amp; "
Priority: " &amp; rf!serviceReqData.priority,
  image: todocument(rf!serviceReqData.relatedCustomer.LogoID),
  timestamp: rf!serviceReqData.requestDateTime
)</definition>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
