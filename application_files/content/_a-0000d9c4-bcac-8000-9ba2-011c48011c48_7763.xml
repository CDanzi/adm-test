        <uuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_7763</uuid>
        <description>Converts a date type variable to a short and descriptive version of date..  example  6/19/2014 becomes Jun, 19</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1981</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=if(#"_a-0000d9a9-eb56-8000-9e06-010000010000_751"(ri!date),null,datetext(ri!date,"MMM dd, yyyy"))</definition>
        <namedTypedValue>
