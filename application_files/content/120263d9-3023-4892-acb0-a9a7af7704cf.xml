    <rule>
        <name>CASE_APP_createNewProduct</name>
        <uuid>120263d9-3023-4892-acb0-a9a7af7704cf</uuid>
        <description>SAIL form for creating a new product for the CASE_APP.</description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5845</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_formLayout" (&#xD;
  label: "Create New Product",&#xD;
  firstColumnContents: {&#xD;
    #"8af7ef67-8654-48e5-bab2-825cea2dae0f"(&#xD;
      product:ri!product,&#xD;
      productImage:ri!productImage,&#xD;
      isCreate:true&#xD;
    )&#xD;
  },&#xD;
  buttons: #"SYSTEM_SYSRULES_ButtonLayout"(&#xD;
    primaryButtons: {&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
          label: "Submit",&#xD;
          style: "PRIMARY",&#xD;
          value: "submit",&#xD;
          saveInto: {&#xD;
            ri!button,&#xD;
            a!save(ri!product.isActive, true),&#xD;
            a!save(ri!product.createdBy, loggedInUser()),&#xD;
            a!save(ri!product.createdOn, now()),&#xD;
            a!save(ri!product.updatedBy, loggedInUser()),&#xD;
            a!save(ri!product.updatedOn, now()),&#xD;
          }&#xD;
      )&#xD;
  },&#xD;
  secondaryButtons: {&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
        label: "Cancel",&#xD;
        value: "cancel",&#xD;
        saveInto: ri!button,&#xD;
        skipValidation: true&#xD;
      )&#xD;
    }&#xD;
  ) &#xD;
)&#xD;
</definition>
