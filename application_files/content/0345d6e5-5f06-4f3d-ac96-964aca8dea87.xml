    <rule>
        <name>CASE_APP_createNewCustomer</name>
        <uuid>0345d6e5-5f06-4f3d-ac96-964aca8dea87</uuid>
        <description>This is the SAIL form for creating a new customer.</description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5839</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(
  local!state: 1,
  local!adminUser:loggedInUser(),
  local!collabUsrList,
  #"SYSTEM_SYSRULES_formLayout"(
    label: "Create New Customer",
    firstColumnContents: {
      #"d3b95f27-f45a-4d97-812f-ef8de3681197"(
        customerData: ri!customerData,
        logo: ri!logo,
        isCreate: ri!isCreate,
        isShippingBillingSame: ri!isShippingBillingSame,
        autoLaunchTasks: ri!autoLaunchTasks,
        /*cancel: ri!cancel,*/
        activeStep: local!state
        )
    },
    buttons: #"SYSTEM_SYSRULES_ButtonLayout"(
      primaryButtons: {
        #"SYSTEM_SYSRULES_ButtonWidget"(
          label: if(local!state=3,"Submit","Next"),
          style: "PRIMARY",
          value:local!state+1,
          saveInto:{
            ri!button,
            a!save(target: local!state,if(local!state=3,3,save!value)),
            a!save(target: ri!customerData.inactiveFlag, value:false),
            /*a!save(target: ri!customerData.accountOwner, local!adminUser),*/
            /*a!save(target: ri!autoLaunchTasks, true)*/
            a!save(ri!customerData.CustomerSince, todate(now())),
            a!save(ri!customerData.CreatedOn, now()),
            a!save(ri!customerData.CreatedBy, loggedInUser()),
          },
          submit:local!state=3,
          validate:true
        )
      },
      secondaryButtons: {
        #"SYSTEM_SYSRULES_ButtonWidget"(
          label: "Go Back",
          style: "NORMAL",
          value:local!state-1,
          saveInto:{
            local!state,
            a!save(target:ri!customerData.inactiveFlag, value:false)
          },
          disabled:local!state=1
        ),
        #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(
          label: "Cancel",
          value: true,
          saveInto: ri!cancel,
          skipValidation: true
        )
      }
    )
  )
)</definition>
