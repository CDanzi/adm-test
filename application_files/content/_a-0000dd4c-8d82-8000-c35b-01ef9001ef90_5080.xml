<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_32654</versionUuid>
    <rule>
        <name>CASE_APP_getProgressForMilestoneCompletion</name>
        <uuid>_a-0000dd4c-8d82-8000-c35b-01ef9001ef90_5080</uuid>
        <description>Based on the milestone task status for an investigation this expression returns the percentage of progress</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2260</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!totalMilestones: count(ri!milestones),
  local!totalCompleted: if(
    #"SYSTEM_SYSRULES_isNullOrEmpty"(ri!milestones),
    0,
    count(wherecontains("Completed", ri!milestones.status))
  ),
  if(
    local!totalMilestones = 0,
    0, 
    tointeger(100 * local!totalCompleted/local!totalMilestones)
  )
)</definition>
        <namedTypedValue>
            <name>milestones</name>
            <type>
                <name>CASE_APP_AdHocTasking?list</name>
                <namespace>urn:com:appian:types:CASE_APP</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
