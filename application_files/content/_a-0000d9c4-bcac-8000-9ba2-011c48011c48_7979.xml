    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_25030</versionUuid>
    <rule>
        <name>CASE_APP_ICM_generateAdHocTasksForIcmData</name>
        <uuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_7979</uuid>
        <description>Generates an array of Ad Hoc Task data based on the milestones in the icmData object passed.</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2260</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= apply(
  #"_a-0000d9c4-bcac-8000-9ba2-011c48011c48_7978"(
    icmData: ri!icmData,
    parentProcessId: ri!parentProcessID,
    entryId: ri!entryId,
    index: _,
    taskType: "Milestone Task"
  ),
  enumerate(length(ri!icmData.milestones)) + 1
)</definition>
        <namedTypedValue>
            <name>icmData</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>parentProcessID</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>entryId</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
