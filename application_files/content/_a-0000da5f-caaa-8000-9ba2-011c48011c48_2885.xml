<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_25087</versionUuid>
    <rule>
        <name>CASE_APP_displayUserOrGroup</name>
        <uuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2885</uuid>
        <description>Displays the User First and Last name or group name depending on the input type.</description>
        <parentUuid>_a-0000da5f-caaa-8000-9ba2-011c48011c48_2880</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=with(
  local!userTypeId:4,
  local!groupTypeId:5,
  if(runtimetypeof(ri!userOrGroup)=4,
    #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_1983"(ri!userOrGroup),
    group(togroup(ri!userOrGroup),"groupName")
  )
)</definition>
        <namedTypedValue>
            <name>userOrGroup</name>
            <type>
                <name>UserOrGroup</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
