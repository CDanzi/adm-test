            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_formLayout"(&#xD;
  label: "Confirm deletion of customer: " &amp; ri!product.name,  &#xD;
  firstColumnContents: {&#xD;
  #"_a-0000d9c4-bcac-8000-9ba2-011c48011c48_4332"(product:ri!product)  &#xD;
  },&#xD;
  buttons: #"SYSTEM_SYSRULES_ButtonLayout"(&#xD;
    primaryButtons: {&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
        label: "Delete",&#xD;
        style: "DESTRUCTIVE",&#xD;
        value: "submit",&#xD;
        confirmMessage:"Are you sure you want to delete this product?",&#xD;
        saveInto: ri!button&#xD;
      ),&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
        label: "Cancel",&#xD;
        skipValidation: true,&#xD;
        value: "cancel",&#xD;
        saveInto: ri!button&#xD;
      )&#xD;
    } &#xD;
  ) &#xD;
)</definition>
