<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_24465</versionUuid>
    <rule>
        <name>CASE_APP_validateEmailAddressFormat</name>
        <uuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1640</uuid>
        <description>Validates email for format in @ symbol, dot placement, and domain</description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5841</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!addressSplit: split(ri!address,"@"),
  local!emailId: local!addressSplit[1],
  local!emailDomain: local!addressSplit[2],
  if(
    length(local!addressSplit)&lt;&gt;2,
    false(),
    not(
      or(
        length(split(local!emailDomain, ".")) &lt; 2,
        contains(split(local!emailId, "."), ""),
        contains(split(local!emailDomain, "."), ""),
        not(isnull(stripwith(lower(local!emailDomain),"abcdefghijklmnopqrstuvwxyz1234567890-."))),
        not(isnull(stripwith(lower(local!emailId),"abcdefghijklmnopqrstuvwxyz1234567890-._+&amp;%")))
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>address</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
