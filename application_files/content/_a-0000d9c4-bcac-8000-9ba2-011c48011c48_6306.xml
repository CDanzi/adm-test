    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_24211</versionUuid>
    <rule>
        <name>CASE_APP_getAllOrderFieldsbyID</name>
        <uuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_6306</uuid>
        <description>Fetches all the fields for an order given the Order id value</description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5996</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isnull(ri!oid),
  null,
    cast(
    'type!{urn:com:appian:types:CASE_APP}CASE_APP_Order',
    #"SYSTEM_SYSRULES_queryEntity"(
      entity: #"_a-0000d9c4-bcac-8000-9ba2-011c48011c48_6305",
      query: #"SYSTEM_SYSRULES_query"(
        filter: #"SYSTEM_SYSRULES_queryFilter"(
          field: "id",
          operator: "=",
          value: ri!oid
        ),
        pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
          startIndex: 1,
          batchSize: 1
        )
      )
    ).data
  )
)</definition>
        <namedTypedValue>
            <name>oid</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
