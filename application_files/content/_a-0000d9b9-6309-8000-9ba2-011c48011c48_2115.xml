<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_24199</versionUuid>
    <rule>
        <name>CASE_APP_getAllCustomerFieldsById</name>
        <uuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_2115</uuid>
        <description>Given a CASE_APP Customer id, get all field values for that customer </description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5841</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!customerData: #"SYSTEM_SYSRULES_queryEntity"(
    entity: #"_a-0000d9b9-6309-8000-9ba2-011c48011c48_2099",
    query: #"SYSTEM_SYSRULES_query"(
      filter: #"SYSTEM_SYSRULES_queryFilter"(
        field: "id",
        operator: "=",
        value: ri!cid
      ),
      pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
        startIndex: 1,
        batchSize: 1
      )
    )
  ),
  if(
    local!customerData.totalCount = 0,
    null,
    cast('type!{urn:com:appian:types:CASE_APP}CASE_APP_Customer', local!customerData.data)
  )
)</definition>
        <namedTypedValue>
            <name>cid</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
