    <rule>
        <name>CASE_APP_documentRowEach</name>
        <uuid>_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6263</uuid>
        <description>Defined a row for the MultiFileUploadGrid</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1981</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_gridRowLayout"(
  index: ri!index,
  contents: {
    #"SYSTEM_SYSRULES_fileUploadField"(
      label: "fileupload " &amp; ri!index,
      required: ri!isRequired,
      target: ri!targetFolder,
      value: ri!uploadedDocs[ri!index],
      saveInto: ri!uploadedDocs[ri!index],
      validations: if(
        #"_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6265"(
          fileImageName: #"SYSTEM_SYSRULES_fileUploadField"(value: ri!uploadedDocs[ri!index]).uploadedDocumentName,
          allowedFileExtensions: ri!allowedFileExtensions
        ),
        null,
        "Only the following extensions are allowed: " &amp; joinarray(ri!allowedFileExtensions, ", ")
      )
    ),
    #"SYSTEM_SYSRULES_imageField"(
      label: "delete " &amp; ri!index,
      images: #"SYSTEM_SYSRULES_documentImage"(
        document: #"SYSTEM_SYSRULES_iconIndicator"("REMOVE"),
        altText: "Remove",
        caption: "Remove",
        link: #"SYSTEM_SYSRULES_dynamicLink"(
          value: ri!index,
          saveInto: {
            a!save(ri!uploadedDocs, remove(ri!uploadedDocs, save!value)),
            a!save(ri!uploadedDocsToken, remove(ri!uploadedDocsToken, save!value))
          }
        )
      ),
      size: "ICON"
    )
  }
)</definition>
        <namedTypedValue>
