    <rule>
        <name>CASE_APP_updateProduct</name>
        <uuid>064fed5a-3dcd-427f-90bb-9c6b5d9c16d1</uuid>
        <description>Initial SAIL form for CASE_APP Update Product process </description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5845</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_formLayout" (&#xD;
  label: "Update Product: "&amp;ri!product.name,&#xD;
  firstColumnContents: {&#xD;
    #"8af7ef67-8654-48e5-bab2-825cea2dae0f"(&#xD;
      product:ri!product,&#xD;
      productImage:ri!productImage,&#xD;
      isCreate:false&#xD;
    )&#xD;
  },&#xD;
  buttons: #"SYSTEM_SYSRULES_ButtonLayout"(&#xD;
    primaryButtons: {&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
          label: "Submit",&#xD;
          style: "PRIMARY",&#xD;
          confirmMessage:"Are you sure?",&#xD;
          value: "submit",&#xD;
          saveInto: ri!button&#xD;
      ),&#xD;
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(&#xD;
          label: "Cancel",&#xD;
          value: "cancel",&#xD;
          saveInto: ri!button,&#xD;
          skipValidation: true&#xD;
      )&#xD;
    }&#xD;
  ) &#xD;
)&#xD;
</definition>
        <namedTypedValue>
            <name>product</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
