<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_25240</versionUuid>
    <rule>
        <name>CASE_APP_MultiFileUploadGrid</name>
        <uuid>_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6262</uuid>
        <description>Allows users to upload multiple files and set their names and descriptions</description>
        <parentUuid>_a-0000d9b9-6309-8000-9ba2-011c48011c48_1981</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>load(
  local!uploadedDocsToken,
  #"SYSTEM_SYSRULES_gridLayout"(
    label: if(
      isnull(ri!gridLabel),
      "Upload Documents",
      ri!gridLabel
    ),
    labelPosition: "ABOVE",
    instructions: if(
      isnull(ri!allowedFileExtensions),
      null,
      "Only the following extensions are allowed: " &amp; joinarray(ri!allowedFileExtensions, ", ")
    ),
    totalcount: if(#"SYSTEM_SYSRULES_isNullOrEmpty"(ri!uploadedDocs), 0, count(ri!uploadedDocs)),
    headerCells: {
      #"SYSTEM_SYSRULES_gridLayoutHeaderCell"(label: "File Upload"),
      #"SYSTEM_SYSRULES_gridLayoutHeaderCell"(label: ""),
    },
    columnConfigs: {
      #"SYSTEM_SYSRULES_gridLayoutColumnConfig"(width: "DISTRIBUTE"),
      #"SYSTEM_SYSRULES_gridLayoutColumnConfig"(width: "ICON")
    },
    rows: #"SYSTEM_SYSRULES_applyComponents"(
      function: #"_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6263"(
        uploadedDocs: ri!uploadedDocs,
        targetFolder: ri!targetFolder,
        index: _,
        uploadedDocsToken: local!uploadedDocsToken,
        isRequired: ri!isRequired
      ),
      array: if(
        #"SYSTEM_SYSRULES_isNullOrEmpty"(ri!uploadedDocs),
        {},
        1 + enumerate(count(ri!uploadedDocs))
      ),
      arrayVariable: local!uploadedDocsToken
    ),
    addrowlink: #"SYSTEM_SYSRULES_dynamicLink"(
      label: "Add Document",
      value: todocument(null),
      saveInto: {
        a!save(ri!uploadedDocs, append(ri!uploadedDocs, save!value)),
        a!save(local!uploadedDocsToken, append(local!uploadedDocsToken, save!value))
      }
    ),
    validations: {}
  )
)</definition>
        <namedTypedValue>
            <name>targetFolder</name>
            <type>
                <name>CollaborationFolder</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>uploadedDocs</name>
            <type>
                <name>CollaborationDocument?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>isRequired</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>gridLabel</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>allowedFileExtensions</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
    <typedValue>
        <type>
            <name>RuleTestConfig?list</name>
            <namespace>http://www.appian.com/ae/types/2009</namespace>
        </type>
        <value>
            <el>
                <a:name/>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">#"_a-0000da5f-caaa-8000-9ba2-011c48011c48_2780"</a:value>
                    <a:nameRef>targetFolder</a:nameRef>
                    <a:id/>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
                    <a:nameRef>uploadedDocs</a:nameRef>
                    <a:id/>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:boolean">false</a:value>
                    <a:nameRef>isRequired</a:nameRef>
                    <a:id/>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xsd:string"/>
                    <a:nameRef>gridLabel</a:nameRef>
                    <a:id/>
                </a:ruleInputTestConfigs>
                <a:ruleInputTestConfigs>
                    <a:value xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="a:Expression">{"jpg","jpeg","png","bpm","gif","xlsx"}</a:value>
                    <a:nameRef>allowedFileExtensions</a:nameRef>
                    <a:id/>
                </a:ruleInputTestConfigs>
                <a:assertions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
            </el>
        </value>
    </typedValue>
    <history>
