<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e0b7-c14e-8000-10cb-01ef9001ef90_32239</versionUuid>
    <rule>
        <name>CASE_APP_getIndexFromListByName</name>
        <uuid>_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6859</uuid>
        <description>Given a unique list of text, and a search term, this rule will return the index of the list where there was a match </description>
        <parentUuid>_a-0000d9c4-bcac-8000-9ba2-011c48011c48_5847</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!match: tostring(filter(
    #"_a-0000dd65-b59d-8000-1a8d-01ef9001ef90_6860"(ri!searchTerm, _),
    ri!testList
  )),
  wherecontains(local!match, ri!testList)
)</definition>
        <namedTypedValue>
            <name>testList</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>searchTerm</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
